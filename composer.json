{
    "name": "videni/piwik-device-detector-bridge",
    "type": "library",
    "description": "piwik device detector bridge for suncat/mobile-detect-bundle ",
    "keywords": ["piwik","device detector","mobile-detect-bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "vidy videni"
        }
    ],
